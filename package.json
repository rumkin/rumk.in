{
  "dependencies": {
    "@hyperapp/render": "^2.0.0",
    "@plant/fs": "^0.1.1",
    "@plant/http": "^1.0.4",
    "@plant/plant": "^2.5.0",
    "escape-html": "^1.0.3",
    "history": "^4.7.2",
    "hyperapp": "^1.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@rollup/plugin-strip": "^1.3.2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-preset-minify": "^0.5.1",
    "rollup": "^1.32.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0"
  },
  "scripts": {
    "clean": "rm -rf build",
    "render": "npm run build && node bin.js / > build/index.html && node bin.js 404 > build/404.html",
    "build": "npm run clean && npm run build:server && npm run build:client && npm run copy",
    "build:dev": "NODE_ENV=development npm run build",
    "copy": "cp -r assets/* build/assets",
    "build:client": "rollup -c rollup.config.js",
    "build:server": "babel src --source-root src --out-dir build",
    "start": "node build/server.js 8000"
  }
}
