{
  "dependencies": {
    "@hyperapp/render": "^2.0.0",
    "@plant/plant": "^1.0.0-alpha.0",
    "fast-memoize": "^2.3.2",
    "history": "^4.7.2",
    "hyperapp": "^1.2.5",
    "mime": "^2.3.1"
  },
  "devDependencies": {
    "webpack": "^2.7.0",
    "webpack-cli": "^2.0.14"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "render": "npm run build && node cli.js / > dist/index.html && node cli.js 404 > dist/404.html",
    "build": "npm run clean && npm run build:dev && cp -r assets/* dist",
    "build:dev": "webpack",
    "build:prod": "NODE_ENV=production webpack",
    "start": "node server.js 8000"
  }
}
