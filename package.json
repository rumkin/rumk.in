{
  "dependencies": {
    "@hyperapp/render": "^2.0.0",
    "@plant/fs": "^0.1.1",
    "@plant/http": "^1.0.3",
    "@plant/plant": "^2.3.0",
    "escape-html": "^1.0.3",
    "fast-memoize": "^2.3.2",
    "history": "^4.7.2",
    "hyperapp": "^1.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@rollup/plugin-json": "^4.0.2",
    "rollup": "^1.32.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0"
  },
  "scripts": {
    "clean": "rm -rf build",
    "render": "npm run build && node cli.js / > build/index.html && node cli.js 404 > build/404.html",
    "build": "npm run clean && npm run compile && npm run copy",
    "copy": "cp -r assets/* build/assets",
    "compile": "npm run rollup",
    "webpack": "webpack",
    "webpack:prod": "NODE_ENV=production npm run compile",
    "rollup": "rollup -c rollup.config.js",
    "rollup:prod": "NODE_ENV=production npm run rollup",
    "start": "node server.js 8000"
  }
}
